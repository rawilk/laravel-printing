{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "types_spaces": {
            "space": "none"
        },
        "combine_consecutive_issets": true,
        "combine_consecutive_unsets": true,
        "declare_parentheses": true,
        "declare_strict_types": true,
        "explicit_string_variable": true,
        "single_trait_insert_per_statement": true,
        "single_line_empty_body": false,
        "ordered_class_elements": {
            "order": [
                "use_trait",
                "case",
                "constant",
                "constant_public",
                "constant_protected",
                "constant_private",
                "property_public",
                "property_protected",
                "property_private",
                "construct",
                "destruct",
                "magic",
                "phpunit",
                "method_abstract",
                "method_public_static",
                "method_public",
                "method_protected_static",
                "method_protected",
                "method_private_static",
                "method_private"
            ],
            "sort_algorithm": "none"
        }
    }
}
